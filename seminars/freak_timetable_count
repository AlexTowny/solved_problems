#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;



int main() {
	int number, week;
	cout << "Enter number and week type:\n";
	cin >> number >> week;
	switch(week){

	case 0:
	switch (number){
	case 1:
		cout << 3;
		break;
	case 2:
		cout << 3;
		break;
	case 3:
		cout << 4;
		break;
	case 4:
		cout << 2;
		break;
	case 5:
		cout << 4;
		break;
	default:
		cout << 0;
		break;
	}
	break;
	case 1:
		switch (number){
		case 1:
			cout << 3;
			break;
		case 2:
			cout << 3;
			break;
		case 3:
			cout << 4;
			break;
		case 4:
			cout << 3;
			break;
		case 5:
			cout << 3;
			break;
		default:
			cout << 0;
			break;
		}
		break;
	}

	return 0;
}
